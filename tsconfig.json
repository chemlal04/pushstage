{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/**/*.ts",
    "**/**/*.tsx",
    ".next/types/**/*.ts",
    "src/lib/prisma.js",
    "src/app/api/User/getStudents/route.ts"
, "src/app/server/db.js", "src/components/Reports/usePagination.ts"  ],
  "exclude": ["node_modules"]
}
